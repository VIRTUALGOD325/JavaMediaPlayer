<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.mediaplayer.javamediaplayer.HelloController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <!-- Username Label -->
        <Label layoutX="50.0" layoutY="150.0" text="Username" />

        <!-- Username TextField -->
        <TextField fx:id="usernameField" layoutX="150.0" layoutY="150.0" />

        <!-- Password Label -->
        <Label layoutX="50.0" layoutY="200.0" text="Password" />

        <!-- Password PasswordField -->
        <PasswordField fx:id="passwordField" layoutX="150.0" layoutY="200.0" />

        <!-- Login Button -->
        <Button layoutX="250.0" layoutY="250.0" onAction="#loginButtonClicked" onMouseClicked="#loginButtonClicked" text="Login" />
    </children>
</AnchorPane>





